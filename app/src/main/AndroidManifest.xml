<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daram">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_darame2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_darame_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SaveActivity"></activity>
        <activity android:name=".SegmentationActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".MosaicActivity" />
        <activity android:name=".CompositeActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>